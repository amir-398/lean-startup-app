{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@authScreens/*": ["src/screens/auth-screens/*"],
      "@mainScreens/*": ["src/screens/main-screens/*"],
      "@navigation/*": ["src/navigation/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["src/constants/*"],
      "@hooks/*": ["src/hooks/*"],
      "@nysaUi/*": ["src/components/nysaUi/*"],
      "@modals/*": ["src/components/modals/*"],
      "@store/*": ["src/store/*"],
      "@types/*": ["src/types/*"],
      "@api/*": ["src/api/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": ["**/*.ts", "**/*.tsx", "nativewind-env.d.ts", "global.d.ts"]
}
